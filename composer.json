{
    "name": "slim/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Lockhart",
            "email": "info@joshlockhart.com",
            "homepage": "http://www.joshlockhart.com/"
        }
    ],
    "require": {
        "php": ">=5.6",
        "andrewdyer/slim3-session-middleware": "^0.1.0",
        "dopesong/slim-whoops": "^2.3",
        "illuminate/database": "^5.4",
        "kriswallsmith/assetic": "^1.4",
        "laravel/helpers": "^1.0",
        "maximebf/debugbar": "^1.15",
        "monolog/monolog": "^1.17",
        "rubellum/slim-blade-view": "^0.1.1",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.1",
        "vlucas/phpdotenv": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Classes\\": "src/app/",
            "Classes\\Controller\\": "src/app/controllers/",
            "Classes\\Model\\": "src/app/models/",
            "Classes\\Lib\\": "src/app/libs/",
            "Classes\\Exception\\": "src/app/exceptions/",
            "App\\Handlers\\": "src/handlers/"
        },
        "files": ["src/app/helpers.php"]
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
